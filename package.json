{
  "name": "pitch-changer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:wasm && npm run build:ts",
    "build:wasm": "./wasm/run-build.sh",
    "build:ts": "eslint ./src/** && tsc && vite build",
    "preview": "vite preview",
    "test": "npm run test:ts && npm run test:wasm && npm run test:ring-buffer",
    "test:ts": "vitest run",
    "test:wasm": "cd wasm && cargo test --release",
    "test:ring-buffer": "node src/ring-buffer.worker-test.ts",
    "clippy": "cd wasm && cargo clippy"
  },
  "dependencies": {
    "@mediabunny/mp3-encoder": "^1.27.3",
    "coi-serviceworker": "^0.1.7",
    "mediabunny": "^1.27.3"
  },
  "devDependencies": {
    "@eslint/css": "^0.14.1",
    "@eslint/js": "^9.39.2",
    "@types/audioworklet": "^0.0.92",
    "@types/node": "^25.2.1",
    "@types/wicg-file-system-access": "^2023.10.7",
    "binaryen": "^125.0.0",
    "eslint": "^9.39.2",
    "jiti": "^2.6.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0",
    "vite": "^7.3.0",
    "vite-plugin-static-copy": "^3.2.0",
    "vitest": "^4.0.16"
  }
}
